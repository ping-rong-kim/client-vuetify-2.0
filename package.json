{
  "name": "DomunClient",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "start": "vue-cli-service serve",
    "serve": "vue-cli-service serve --mode production",
    "remote": "vue-cli-service serve --mode remote",
    "remote-go": "vue-cli-service serve --mode remote-go",
    "local-mortgagecomposer-remote-go": "vue-cli-service serve --mode local-mortgagecomposer-remote-go",
    "local-standard": "vue-cli-service serve --mode local-standard",
    "remote-local-go": "vue-cli-service serve --mode remote-local-go",
    "serve-domun-local": "vue-cli-service serve --mode domun-local",
    "build": "vue-cli-service build --dest dist/production",
    "build:domun-production": "vue-cli-service build  --mode domun-production --dest dist/domun/production",
    "build:dev": "vue-cli-service build --mode dev --dest dist/dev",
    "build:qa": "vue-cli-service build  --mode qa --dest dist/qa",
    "build:stage": "vue-cli-service build  --mode stage --dest dist/stage",
    "build:demo": "vue-cli-service build  --mode demo --dest dist/demo",
    "build:prod": "vue-cli-service build  --mode prod --dest dist/prod",
    "build:live": "vue-cli-service build  --mode live --dest dist/live",
    "build:qa:mortgagecomposer": "vue-cli-service build  --mode qa.mortgagecomposer --dest dist/qa",
    "lint": "vue-cli-service lint",
    "lint-fix": "node_modules\\.bin\\eslint  --fix-type suggestion,layout,problem --fix . --ext .js,.vue src",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "all": "concurrently \"cd ..\\server && npm run start\" \"cd ..\\dashboard && npm run start\" \"npm run start\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@bugsnag/js": "^6.5.2",
    "@bugsnag/plugin-vue": "^6.5.0",
    "@saeris/vue-spinners": "^1.0.8",
    "@vue/babel-preset-app": "^4.5.6",
    "aos": "^3.0.0-beta.6",
    "apexcharts": "^3.20.2",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.9.3",
    "chartjs-adapter-moment": "^0.1.2",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "format-currency": "^1.0.0",
    "js-cookie": "^2.2.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.20",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.28.0",
    "nodemailer": "^6.4.11",
    "ordinal-js": "^1.1.0",
    "prerender-spa-plugin": "^3.4.0",
    "register-service-worker": "^1.7.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "swiper": "^5.4.5",
    "vee-validate": "^2.2.11",
    "vue": "^2.6.12",
    "vue-analytics": "^5.22.1",
    "vue-apexcharts": "^1.6.0",
    "vue-awesome-swiper": "^4.1.1",
    "vue-chartjs": "^3.5.1",
    "vue-cli-plugin-prerender-spa": "^1.1.6",
    "vue-confirm-dialog": "^1.0.2",
    "vue-cookie-law": "^1.13.3",
    "vue-head": "^2.2.0",
    "vue-router": "^3.4.3",
    "vue-router-sitemap": "0.0.4",
    "vue-scrollto": "^2.18.2",
    "vue-social-sharing": "^2.4.7",
    "vue2-circle-progress": "^1.2.3",
    "vuetify": "^2.5.0",
    "vuetify-confirm": "^2.0.6",
    "vuetify-loader": "^1.6.0",
    "vuex": "^3.5.1",
    "vuex-persist": "^2.3.0",
    "vuex-router-sync": "^5.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@mdi/font": "^4.9.95",
    "@vue/cli-plugin-e2e-cypress": "^3.12.1",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-unit-jest": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^5.2.3",
    "g": "^2.0.1",
    "lint-staged": "^8.1.6",
    "material-design-icons-iconfont": "^4.0.5",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.2",
    "raw-loader": "^3.1.0",
    "sass": "^1.26.11",
    "sass-loader": "^7.1.0",
    "vue-eslint-parser": "^6.0.5",
    "vue-template-compiler": "^2.6.12",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
